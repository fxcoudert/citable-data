+void InitializeLambda(void)
+{
+  int i;
+  
+  Lambda    = (REAL*)calloc(AlchReacLambda+1,sizeof(REAL));
+  
+  for(i=0;i<=AlchReacLambda;i++)
+  {
+	Lambda[i]    = ((REAL)i)/AlchReacLambda;
+  }
+}
